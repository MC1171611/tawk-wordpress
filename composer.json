{
    "name": "tawk/tawk-wordpress",
    "description": "Wordpress plugin for tawk.to",
    "type": "project",
    "license": "OSL-3",
    "version": "0.6.0",
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6",
        "wp-coding-standards/wpcs": "^2.3",
        "phpcompatibility/php-compatibility": "^9.3",
        "woocommerce/woocommerce-sniffs": "^0.1.1"
    },
    "scripts": {
        "post-install-cmd": "phpcs --config-set installed_paths vendor/wp-coding-standards/wpcs,vendor/phpcompatibility/php-compatibility",
        "phpcs": "phpcs -p -s -v -n tawkto",
        "phpcbf": "phpcbf -p -s -v -n tawkto"
    }
}
